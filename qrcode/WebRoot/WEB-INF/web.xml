<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>qrcode</display-name>
	<servlet>
		<servlet-name>QrcodeController</servlet-name>
		<servlet-class>com.ykx.qr.QrcodeController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImgUpload</servlet-name>
		<servlet-class>com.ykx.qr.ImgUpload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Content</servlet-name>
		<servlet-class>com.ykx.qr.Content</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>QrcodeController</servlet-name>
		<url-pattern>/qrcode</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImgUpload</servlet-name>
		<url-pattern>/ImgUpload</url-pattern>
	</servlet-mapping>
</web-app>